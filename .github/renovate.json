{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "prHourlyLimit": 0,
    "repositories": [
        "rebellions-sw/optimum-rbln"
    ],
    "lockFileMaintenance": {
        "enabled": true
    },
    "prTitle": "dependency: Update {{depName}} to {{newVersion}}",
    "packageRules": [
        {
            "matchPackageNames": [
                "*"
            ],
            "enabled": false
        },
        {
            "matchDatasources": [
                "pypi"
            ],
            "matchDepTypes": [
                "dependencies",
                "optionalDependencies"
            ],
            "matchPackageNames": [
                "diffusers"
            ],
            "enabled": true,
            "groupName": "huggingface-diffusers",
            "prTitle": "dependency: Update {{depName}} to {{newVersion}}"
        },
        {
            "matchDatasources": [
                "pypi"
            ],
            "matchDepTypes": [
                "dependencies",
                "optionalDependencies"
            ],
            "matchPackageNames": [
                "transformers"
            ],
            "enabled": true,
            "groupName": "huggingface-transformers",
            "prTitle": "dependency: Update {{depName}} to {{newVersion}}"
        }
    ]
}