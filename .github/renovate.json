{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "*"
      ],
      "enabled": false
    },
    {
      "matchDatasources": [
        "pypi"
      ],
      "matchPackageNames": [
        "diffusers"
      ],
      "enabled": true,
      "groupName": "huggingface-diffusers"
    },
    {
      "matchDatasources": [
        "pypi"
      ],
      "matchPackageNames": [
        "transformers"
      ],
      "enabled": true,
      "groupName": "huggingface-transformers"
    }
  ]
}